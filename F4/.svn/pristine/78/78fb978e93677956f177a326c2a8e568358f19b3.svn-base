		<meta charset='utf-8'>
		<meta http-equiv="Cache-Control" content="no-siteapp" />
		<title>评价</title>
		<meta name="viewport" content="width=device-width,initial-scale=1.0, maximum-scale=1.0, user-scalable=no">
		<meta http-equiv="Cache-Control" content="no-siteapp">
		<meta name="apple-mobile-web-app-capable" content="yes">
		<meta name="mobile-web-app-capable" content="yes">
		<meta name="format-detection" content="telephone=no">
		<meta name="keywords" content="">
		<meta name="description" content="">
		<link rel="stylesheet" type="text/css" href="__STATIC__/index/css/base.css" />
		<link rel="stylesheet" type="text/css" href="__STATIC__/index/css/common.css" />
		<script src="__STATIC__/index/js/jquery-1.8.3.min.js" type="text/javascript"></script>
		<script type="text/javascript" src="__STATIC__/index/js/jquery.SuperSlide.2.1.js"></script>
	</head>

	<body>
		<!--header star-->
		{include file="static/index/menu/head.html"}
		<!--navbar end-->

		<!--Main-wrap-->
		<div class="main-wrap graybg">
			<div class="wrapper pt40 pb40 clearfix">
				{include file="static/index/menu/gerenxinxi.html"}
				<div class="fr slide-show white-box">
					<div class="dark-tit line-bot clearfix">
						<h3 class="fl fs26">评价晒单</h3>
					</div>
					<div class="goods-pj">
						<div class="fl pj-goods">
							<a href="#" class="pro"><span><img src="__STATIC__/index/upload/2.jpg" alt=""/></span></a>
							<a href="#" class="tit fs14 full-db darkgray">北京椿龄文化墙角一枝梅</a>
							<span class="fs14 full-db"><em class="orange">29.90</em> 元</span>
							<span class="fs12 full-db">95127人评价</span>
						</div>
						<div class="fr pj-info">
							<div id="xzw_starSys" class="mt20 clearfix">
								<div id="xzw_starBox" class="fl">
									<ul class="star" id="star">
										<li>
											<a href="javascript:void(0)" title="1" class="one-star">1</a>
										</li>
										<li>
											<a href="javascript:void(0)" title="2" class="two-stars">2</a>
										</li>
										<li>
											<a href="javascript:void(0)" title="3" class="three-stars">3</a>
										</li>
										<li>
											<a href="javascript:void(0)" title="4" class="four-stars">4</a>
										</li>
										<li>
											<a href="javascript:void(0)" title="5" class="five-stars">5</a>
										</li>
									</ul>
									<div class="current-rating" id="showb"></div>
								</div>
								<div class="fl description ml10 fs14"></div>
							</div>
							<div class="pj-cont mt15">
								<form action="">
									<textarea name="" id="" cols="30" rows="10" placeholder="色泽如何，口感如何，谈谈你的感受吧！"></textarea>
									<div class="pj-but tr mt15">
										<input type="button" onMouseMove="f.style.pixelLeft=event.x-60;f.style.pixelTop=this.offsetTop;" value="添加图片" onClick="f.click()" class="add_pic fs14">
										<input type="file" id="f" onChange="txt.value=this.value" name="f" style="height:26px;" class="files" size="1" hidefocus>

										<input type="button" class="orange-but fs14 ml10" value="提交评价">
									</div>
								</form>
							</div>

						</div>
					</div>
				</div>

			</div>
		</div>
		<!--/end-->

		<!--footer star-->
		{include file="static/index/menu/foot.html"}
		<!--footer end-->
	</body>

</html>